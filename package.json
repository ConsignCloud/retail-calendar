{
  "name": "merchandise-calendar",
  "version": "1.0.0",
  "description": "A configurable merchandise calendar",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint --fix -p tsconfig.json",
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build",
    "prepublish": "tsc",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "merchandise",
    "calendar",
    "retail"
  ],
  "author": "cihan@toolio.com",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "moment": "^2.25.3"
  },
  "engines": {
    "node": "~10.13.0"
  },
  "volta": {
    "node": "10.13.0"
  }
}
